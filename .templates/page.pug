block var

doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		title ICPC World Finals Astana
		link(rel="stylesheet", href="assets/styles/main.css")
		link(rel="icon" type="image/x-icon" href="logo/favicon.ico")
		meta(property="og:title" content="2024 ICPC World Finals Astana")
		meta(property="og:description" content="The 48th Annual World Championship of the ICPC International Collegiate Programming Contest")
		meta(property="og:type" content="website")
		meta(property="og:image" content="/og-image.png")
	body
		include ../assets/img/sprite.svg

		nav.mainmenu
			.mainmenu__list
				a.mainmenu__item(class = (activeMenu == 'home') ? 'mainmenu__item--current' : '')(href="index.html") Home 
				a.mainmenu__item(class = (activeMenu == 'about') ? 'mainmenu__item--current' : '')(href="about.html") About ICPC
				a.mainmenu__item(class = (activeMenu == 'participants') ? 'mainmenu__item--current' : '')(href="for-participants.html") For participants 
				a.mainmenu__item(class = (activeMenu == 'challenge') ? 'mainmenu__item--current' : '')(href="challenge.html") Challenge
				a.mainmenu__item(class = (activeMenu == 'astana') ? 'mainmenu__item--current' : '')(href="astana.html") Astana 
				a.mainmenu__item(class = (activeMenu == 'venue') ? 'mainmenu__item--current' : '')(href="venue.html") Venue 
				a.mainmenu__item(href="https://news.icpc.global/gallery" target="_blank") Gallery
					svg: use(xlink:href="#external")
				a.mainmenu__item(class = (activeMenu == 'excursions') ? 'mainmenu__item--current' : '')(href="excursions.html") Excursions 
				a.mainmenu__item(href="https://docs.icpc.global/wf-schedule/" target="_blank") Schedule
					svg: use(xlink:href="#external")
				a.mainmenu__item(class = (activeMenu == 'organizers') ? 'mainmenu__item--current' : '')(href="organizers.html") Host and Organizers
			button.mainmenu__toggle Menu
			div.mainmenu__overlay

		block body

		footer.footer
			div.footer__wrap
				div.footer__col
					p.footer__copyright
						| Â© 2024, ICPC.
						br
						| All rights are reserved.
					p.footer__external
						a(href="https://icpc.global/" target="_blank") icpc.global
						br
						a(href="https://icpc.foundation/" target="_blank") icpc.foundation

				div.footer__slogan
					div.footer__slogan-content
						| #[span.color-r while](#[span.color-y alive]) {
						br
						| &emsp; #[span.color-b think]();
						br
						| &emsp; #[span.color-b create]();
						br
						| &emsp; #[span.color-b solve]();
						br
						| }
						span.footer__slogan-carret |

				div.footer__col
					div.footer__socials
						a(href="https://www.facebook.com/ICPCNews" target="_blank"): svg: use(xlink:href="#facebook")
						a(href="https://twitter.com/ICPCNews" target="_blank"): svg: use(xlink:href="#twitter")
						a(href="https://www.instagram.com/ICPCNews/" target="_blank"): svg: use(xlink:href="#instagram")
						a(href="https://www.linkedin.com/groups/1872736/" target="_blank"): svg: use(xlink:href="#linkedin")
						a(href="https://www.youtube.com/user/ICPCNews" target="_blank"): svg: use(xlink:href="#youtube")
						a(href="https://vk.com/icpcnews" target="_blank"): svg: use(xlink:href="#vk")
						a(href="https://t.me/ICPCNews" target="_blank"): svg: use(xlink:href="#telegram")

					div.footer__contacts
						a(href="mailto:manager@icpc.global" target="_blank") manager@icpc.global
			
		script(src="assets/scripts/swiper-bundle.min.js")
		script(src="assets/scripts/countUp.min.js" type="module")
		script(src="assets/scripts/main.js" type="module")